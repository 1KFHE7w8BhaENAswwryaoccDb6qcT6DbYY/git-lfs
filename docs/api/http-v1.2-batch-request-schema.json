{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Git LFS HTTPS Batch API v1.2 Request",
  "type": "object",
  "properties": {
    "operation": {
      "type": "string"
    },
    "objects": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "oid": {
            "type": "string"
          },
          "size": {
            "type": "number"
          }
        },
        "required": ["oid", "size"],
        "additionalProperties": false
      }
    },
    "ref": {
      "type": "string"
    }
  },
  "required": ["objects", "operation"],
  "additionalProperties": false
}
